# Snippets for coding in C#
snippet if_statement
alias if      
options head
    if (${1:condition})
    {
        ${2:block}
    }
    ${0}

snippet simple_for_loop_counter
alias fo      
options head
    for (int ${1:i} = 0; $1 < ${2:range}; ++$1)
    {
        ${0}
    }

snippet general_for_setup
alias for     
options head
    for (${1:initialize}; ${2:test}; ${3:alter})
    {
        ${0}
    }

snippet do-while_loop
alias do      
options head
    do
    {
        ${0}
    } while (${1:test});

snippet foreach
alias fe      
options head
    foreach (${1:var} in ${2:collection})
    {
        ${3:loop body}
    }
    ${0}

