snippet private_property_-_full
alias prop    
    private $1 `!p snip.rv = '_' + ''.join(t[2].[0].lower() + t[2][1:])`
    private ${1:Type} ${2:Name}
    {
        get
        {
            return `!p snip.rv = '_' + ''.join(t[2].[0].lower() + t[2][1:])`;
        }
        set
        {
            `!p snip.rv = '_' + ''.join(t[2].[0].lower() + t[2][1:])` = value;
        }
    }

