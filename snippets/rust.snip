snippet pub_mod
alias pmod    
options head
    pub mod ${1:`substitute(vim_snippets#Filename(), '\(_\|^\)\(.\)', '\u\2', 'g')`} {
        ${0}
    } /* $1 */

snippet struct
alias st      
options head
    struct ${1: StructNamee} {
        ${0},
    } /* $1 */

snippet pub_struct
alias pst     
options head
    pub struct ${1: PubStructNamee} {
        ${0},
    } /* $1 */

# loops
snippet loop_a_number_of_times
alias fr_     
    for _ in &[0..${1:N}] {
        ${0}
    }

snippet loop_with_index
alias fri     
    for i in &[0..${1:N}] {
        ${0: Block that needs an index i}
    }

# comments
snippet line_comment
alias //      
    // ${1:Line Comment}
    ${0}

snippet wrap_comment
alias /*      
    /* ${1:Wrap Comment} */${0}

snippet block_comment
alias /**     
    /*
     * ${1:Block Comment}
     */
    ${0}

snippet fixme
alias fm      
    // FIXME: $1

snippet XXX:
alias xxx    
    // XXX: $1

snippet todo
alias td      
    // TODO: $1

# macros
snippet println!_macro
alias !pl     
options head
    println!("${1:format string}");${0}

snippet println!_macro_with_args
alias !pl,    
options head
    println!("${1:format string}", ${2:args});${0}

snippet print!_macro
alias !p      
options head
    print!("${1:format string}");${0}

snippet print!_macro_with_args
alias !p,     
options head
    print!("${1:format string}", ${2:args});${0}

snippet writeln!_macro
alias !wl     
options head
    writeln!(${1:buffer}, "${2:format string}").unwrap();${0}

snippet writeln!_macro_with_args
alias !wl,    
options head
    writeln!(${1:buffer}, "${2:format string}", ${3:args}).unwrap();${0}

snippet write_macro
alias !w      
options head
    write!(${1:buffer}, "${2:format string}").unwrap();${0}

snippet write!_macro_with_args
alias !w,     
options head
    write!(${1:buffer}, "${2:format string}", ${3:args}).unwrap();${0}

snippet format!_macro
alias !f      
options word
    format!("${1:format string}")${0}

snippet format!_macro_with_args
alias !f,     
options word
    format!("${1:format string}", ${2:args})${0}

snippet vec!_macro
alias !v      
options word
    vec![${1:items}]${0}

snippet assert!_macro
alias !as     
options head
    assert!(${1:assertion})${0}

snippet assert!_macro_with_custom_message
alias !ass    
options head
    assert!(${1:assertion}, "${2:message}")${0}

snippet assert!_macro_with_custom_message_with_args
alias !as,    
options head
    assert!(${1:assertion}, "${2:message}", ${3:message args})${0}

snippet assert_eq!_macro
alias !ae     
options head
    assert_eq!(${1:val1}, ${2:val2})${0}

snippet assert_eq!_macro_with_custom_message
alias !aee    
options head
    assert_eq!(${1:val1}, ${2:val2}, "${3:message}")${0}

snippet assert_eq!_macro_with_custom_message_with_args
alias !ae,    
options head
    assert_eq!(${1:val1}, ${2:val2}, "${3:message}", ${4:message args})${0}

